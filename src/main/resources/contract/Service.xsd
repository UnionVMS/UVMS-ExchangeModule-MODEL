<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:service="urn:exchange.schema.fisheries.ec.europa.eu:v1" 
            targetNamespace="urn:exchange.schema.fisheries.ec.europa.eu:v1"
            xmlns:exchange="urn:exchange.schema.fisheries.ec.europa.eu:v1"
            xmlns:common="urn:common.exchange.schema.fisheries.ec.europa.eu:v1"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
            xmlns="http://schemas.xmlsoap.org/wsdl/">
    
    <xsd:complexType name="ServiceType">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:string"/>
            <xsd:element name="serviceClassName" type="xsd:string"/>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="description" type="xsd:string"/>
            <xsd:element name="updated" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    
      
    <xsd:complexType name="SettingType">
        <xsd:complexContent>
            <xsd:extension base="common:keyValueType"/>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="SettingListType">
        <xsd:sequence>
            <xsd:element name="setting" type="exchange:SettingType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="CapabilityType">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="CapabilityListType">
        <xsd:sequence>
            <xsd:element name="capability" type="exchange:CapabilityType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    

</xsd:schema>